local module = {}
local UIS = game:GetService("UserInputService")
function module:R6(target:Player)
	repeat wait() until game:IsLoaded()
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-1.5,1,-1.5)
		att1.Rotation = Vector3.new(0, 0, 0)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false

		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end

	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local Hats = {
		xd = Character:WaitForChild("Pal Hair"),
		xd2 = Character:WaitForChild("InternationalFedora"),
		xd3 = Character:WaitForChild("MeshPartAccessory")
	}
	Hats.xd.Handle.AccessoryWeld:Remove()
	Hats.xd.Handle.Mesh:Remove()
	Hats.xd2.Handle.AccessoryWeld:Remove()
	Hats.xd2.Handle.SpecialMesh:Remove()
	Hats.xd3.Handle.AccessoryWeld:Remove()
	Hats.xd3.Handle.SpecialMesh:Remove()
	local function align(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(0,-1,1)
		att1.Name = "PosAtt1"
		game:GetService("RunService").Heartbeat:Connect(function()
			att1.Rotation = att1.Rotation + Vector3.new(0,0,0)
		end)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false

		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	align(Hats.xd.Handle, target.Character["Torso"])
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align2(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-1.5,1,-1.5)
		att1.Rotation = Vector3.new(0, 0, 0)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true

		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999

		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align2(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(0.6, -1.5, 1.7)
		att1.Orientation = Vector3.new(0,90,0)
		att1.Name = "PosAtt2"
		game:GetService("RunService").Heartbeat:Connect(function()
			att1.Rotation = att1.Rotation + Vector3.new(0,0,0)
		end)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1

		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	align2(Hats.xd2.Handle, target.Character["Torso"])
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align3(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-1.5,1,-1.5)
		att1.Rotation = Vector3.new(0, 0, 0)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align3(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-0.6, -1.5, 1.7)
		att1.Orientation = Vector3.new(0,90,0)
		att1.Name = "PosAtt3"
		game:GetService("RunService").Heartbeat:Connect(function()
			att1.Rotation = att1.Rotation + Vector3.new(0,0,0)
		end)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	align3(Hats.xd3.Handle,target.Character["Torso"])
	UIS.InputBegan:Connect(function(Input)
		pcall(function()
			if Input.KeyCode == Enum.KeyCode.E then
				if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
					target.Character.Torso.PosAtt1.Position = Vector3.new(0,-1,2)
					target.Character.Torso.PosAtt2.Position = Vector3.new(0.6, -1.5, 2.7)
					target.Character.Torso.PosAtt3.Position = Vector3.new(-0.6, -1.5, 2.7)
				end
			elseif Input.KeyCode == Enum.KeyCode.Q then
				if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
					target.Character.Torso.PosAtt1.Position = Vector3.new(0,-1,1)
					target.Character.Torso.PosAtt2.Position = Vector3.new(0.6, -1.5, 1.7)
					target.Character.Torso.PosAtt3.Position = Vector3.new(-0.6, -1.5, 1.7)
				end
			elseif Input.KeyCode == Enum.KeyCode.R then
				if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
					target.Character.Torso.PosAtt1.Position = Vector3.new(0, 1.1, -1) --0.6, 0.9, -2
					target.Character.Torso.PosAtt2.Position = Vector3.new(0.6, 0.9, -2)
					target.Character.Torso.PosAtt3.Position = Vector3.new(-0.6, 0.9, -2)
				end

			elseif Input.KeyCode == Enum.KeyCode.T then
				if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
					target.Character.Torso.PosAtt1.Position = Vector3.new(0, 1.1, -2) --0.6, 0.9, -2
					target.Character.Torso.PosAtt2.Position = Vector3.new(0.6, 0.9, -3)
					target.Character.Torso.PosAtt3.Position = Vector3.new(-0.6, 0.9, -3)
				end

			end
		end)
	end)


end
function module:R15(target:Player)
	repeat wait() until game:IsLoaded()

	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-1.5,1,-1.5)
		att1.Rotation = Vector3.new(0, 0, 0)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character




	local Hats = {
		xd = Character:WaitForChild("Pal Hair"),
		xd2 = Character:WaitForChild("InternationalFedora"),
		xd3 = Character:WaitForChild("MeshPartAccessory")
	}
	Hats.xd.Handle.AccessoryWeld:Remove()
	Hats.xd.Handle.Mesh:Remove()
	Hats.xd2.Handle.AccessoryWeld:Remove()
	Hats.xd2.Handle.SpecialMesh:Remove()
	Hats.xd3.Handle.AccessoryWeld:Remove()
	Hats.xd3.Handle.SpecialMesh:Remove()
	local function align(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(0,-1,2)
		att1.Name = "PosAtt1"
		game:GetService("RunService").Heartbeat:Connect(function()
			att1.Rotation = att1.Rotation + Vector3.new(0,0,0)
		end)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	align(Hats.xd.Handle, target.Character["HumanoidRootPart"])









	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align2(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-1.5,1,-1.5)
		att1.Rotation = Vector3.new(0, 0, 0)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character




	local function align2(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(0.6, -1.5, 2.7)
		att1.Orientation = Vector3.new(0,90,0)
		att1.Name = "PosAtt2"
		game:GetService("RunService").Heartbeat:Connect(function()
			att1.Rotation = att1.Rotation + Vector3.new(0,0,0)
		end)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	align2(Hats.xd2.Handle, target.Character["HumanoidRootPart"])











	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character
	local function align3(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-1.5,1,-1.5)
		att1.Rotation = Vector3.new(0, 0, 0)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000
	end
	local Plr = game:GetService("Players").LocalPlayer
	local Player = game:GetService("Players").LocalPlayer
	local Character = Player.Character




	local function align3(i,v)
		local att0 = Instance.new("Attachment", i)
		att0.Position = Vector3.new(0,0,0)
		local att1 = Instance.new("Attachment", v)
		att1.Position = Vector3.new(-0.6, -1.5, 2.7)
		att1.Orientation = Vector3.new(0,90,0)
		att1.Name = "PosAtt3"
		game:GetService("RunService").Heartbeat:Connect(function()
			att1.Rotation = att1.Rotation + Vector3.new(0,0,0)
		end)
		local AP = Instance.new("AlignPosition", i)
		AP.Attachment0 = att0
		AP.Attachment1 = att1
		AP.RigidityEnabled = false
		AP.ReactionForceEnabled = false
		AP.ApplyAtCenterOfMass = true
		AP.MaxForce = 9999999
		AP.MaxVelocity = math.huge
		AP.Responsiveness = 1000000000000
		local AO = Instance.new("AlignOrientation", i)
		AO.Attachment0 = att0
		AO.Attachment1 = att1
		AO.ReactionTorqueEnabled = true
		AO.PrimaryAxisOnly = false
		AO.MaxTorque = 9999999
		AO.MaxAngularVelocity = math.huge
		AO.Responsiveness = 10000000000000


	end

	align3(Hats.xd3.Handle, target.Character["HumanoidRootPart"])
	UIS.InputBegan:Connect(function(Input)
		if Input.KeyCode == Enum.KeyCode.E then
			if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
				target.Character.UpperTorso.PosAtt1.Position = Vector3.new(0,-1,2)
				target.Character.UpperTorso.PosAtt2.Position = Vector3.new(0.6, -1.5, 2.7)
				target.Character.UpperTorso.PosAtt3.Position = Vector3.new(-0.6, -1.5, 2.7)
			end

		elseif Input.KeyCode == Enum.KeyCode.Q then

			if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
				target.Character.UpperTorso.PosAtt1.Position = Vector3.new(0,-1,1)
				target.Character.UpperTorso.PosAtt2.Position = Vector3.new(0.6, -1.5, 1.7)
				target.Character.UpperTorso.PosAtt3.Position = Vector3.new(-0.6, -1.5, 1.7)
			end
		elseif Input.KeyCode == Enum.KeyCode.R then

			if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
				target.Character.UpperTorso.PosAtt1.Position = Vector3.new(0, 1.1, -1)
				target.Character.UpperTorso.PosAtt2.Position = Vector3.new(0.6, 0.9, -2)
				target.Character.UpperTorso.PosAtt3.Position = Vector3.new(-0.6, 0.9, -2)
			end

		elseif Input.KeyCode == Enum.KeyCode.T then

			if target.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
				target.Character.UpperTorso.PosAtt1.Position = Vector3.new(0, 1.1, -2)
				target.Character.UpperTorso.PosAtt2.Position = Vector3.new(0.6, 0.9, -3)
				target.Character.UpperTorso.PosAtt3.Position = Vector3.new(-0.6, 0.9, -3)
			end

		end
	end)


end
return module
